<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Proyecto_Integrador.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Proyecto_Integrador.View"
    Shell.FlyoutBehavior="Locked"
    Shell.BackgroundColor="#515151"
    Shell.FlyoutWidth="250"
    FlyoutHeaderBehavior="CollapseOnScroll">

    <FlyoutItem Title="Home">
        <ShellContent ContentTemplate="{DataTemplate local:MainPage}"/>
    </FlyoutItem>
    <FlyoutItem Title="Historial">
        <ShellContent ContentTemplate="{DataTemplate local:Historial}"/>
    </FlyoutItem>

</Shell>
